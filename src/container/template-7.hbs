{{> headerDark data=header}}

<div class="hero">
	{{#with hero}}
	<h1 class="hero_title">{{title}}</h1>
	<p class="hero_text">{{description}}</p>
	<a href="{{button.url}}" class="hero_button">{{button.text}}</a>
	{{/with}}
</div>

<div class="about">
	{{#with about}}
	<h3 class="heading">{{heading}}</h3>
	<p class="info">{{info}}</p>
	{{/with}}
</div>

{{> articleCardDark data=articles}}


{{> footerDark data=footer}}